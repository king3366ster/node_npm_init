{
  "name": "node_npm_init",
  "version": "1.0.0",
  "description": "frontend npm init framework",
  "main": "index.js",
  "scripts": {
    "test": "npm test",
    "clean": "run-p -lns clean:*",
    "clean:css": "rimraf webapp/dist/css",
    "clean:js": "rimraf webapp/dist/js",
    "build": "run-p -lns build:*",
    "build:css": "postcss --config webapp/build/postcss.js --progress --silent --colors --watch",
    "build:js": "webpack --config webapp/build/webpack.config.js --progress --silent --watch",
    "dev": "run-s -lns clean build"
  },
  "keywords": [
    "frontend",
    "postcss",
    "webpack"
  ],
  "author": "king3366ster",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel": "^6.5.2",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.14.0",
    "cssnano": "^3.7.5",
    "cssnext": "^1.8.4",
    "globby": "^6.0.0",
    "jsx-loader": "^0.13.2",
    "npm-run-all": "^3.1.0",
    "postcss": "^5.2.0",
    "postcss-calc": "^5.3.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-mixins": "^5.2.0",
    "postcss-nested": "^1.0.0",
    "postcss-scss": "^0.3.0",
    "postcss-simple-vars": "^3.0.0",
    "precss": "^1.4.0",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.2"
  }
}
