{
  "name": "node_npm_init",
  "version": "2.0.0",
  "description": "frontend npm init framework",
  "main": "index.js",
  "scripts": {
    "start": "run-p -lns ",
    "test": "npm test",
    "clean": "run-p -lns clean:*",
    "clean:css": "rimraf webapp/dist/css",
    "clean:js": "rimraf webapp/dist/js",
    "build": "run-p -lns build:*",
    "build:css": "postcss webapp/src/css/**/*.css webapp/src/css/*.css -c webapp/build/postcss.config.js -d webapp/dist/css --poll --watch",
    "build:js": "node webapp/build/build.webpack.js",
    "watch": "run-p watch:*",
    "watch:css": "npm run build:css --progress --silent -- --watch",
    "watch:js": "npm run build:js --progress --silent -- --watch",
    "dev": "npm run watch",
    "dev:t": "run-s -lns clean build",
    "server": "node backend/require.js"
  },
  "keywords": [
    "frontend",
    "postcss",
    "webpack"
  ],
  "author": "king3366ster",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "axios": "^0.14.0",
    "babel": "^6.5.2",
    "babel-core": "^6.14.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.14.0",
    "css-loader": "^0.25.0",
    "cssnano": "^3.7.5",
    "globby": "^6.0.0",
    "lodash": "^4.16.1",
    "npm-run-all": "^4.0.2",
    "postcss": "^5.2.16",
    "postcss-clean": "^1.0.2",
    "postcss-cli": "^4.0.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-scss": "^0.4.1",
    "precss": "^1.4.0",
    "vue": "^2.2.4",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^12.2.2",
    "vue-router": "^2.7.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.1",
    "webpack": "^3.2.0"
  },
  "dependencies": {
    "babel-preset-env": "^1.6.0",
    "fs-extra": "^4.0.0",
    "koa": "^2.3.0",
    "koa-bodyparser": "^3.2.0",
    "koa-mount": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-send": "^3.2.0",
    "koa-session": "^3.3.1",
    "koa-static": "^3.0.0"
  }
}
